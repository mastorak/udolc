<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires about_udolc_dialog 1.0 -->
  <!-- interface-requires gtk+ 3.0 -->
  <object class="AboutUdolcDialog" id="about_udolc_dialog">
    <property name="can_focus">False</property>
    <property name="border_width">5</property>
    <property name="icon">../media/udolc.svg</property>
    <property name="type_hint">normal</property>
    <property name="program_name">Unity Dock Launcher Creator (Udolc)</property>
    <property name="version">0.1</property>
    <property name="copyright" translatable="yes">Copyright Â© 2013 Konstantinos Mastorakis</property>
    <property name="comments" translatable="yes">A utlity to create launchers for Ubuntu Unity</property>
    <property name="license" translatable="yes">GPL 3</property>
    <property name="authors">Konstantinos Mastorakis &lt;mastorak@gmail.com&gt;</property>
    <property name="logo">../media/udolc.svg</property>
    <child internal-child="vbox">
      <object class="GtkBox" id="dialog-vbox1">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <child internal-child="action_area">
          <object class="GtkButtonBox" id="dialog-action_area1">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="layout_style">end</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <placeholder/>
        </child>
      </object>
    </child>
  </object>
</interface>
